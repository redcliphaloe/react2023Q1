{
  "name": "components",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://redcliphaloe.github.io/react2023Q1/",
  "scripts": {
    "dev": "vite",
    "build": "npm run lint && tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint --ext js,ts,tsx src",
    "lint:fix": "eslint --fix --ext js,ts,tsx src",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "deploy": "npm run build && gh-pages -d dist",
    "dev:ssr": "nodemon",
    "build:ssr": "npm run build:ssr:client && npm run build:ssr:server",
    "build:ssr:client": "vite build --outDir dist/client",
    "build:ssr:server": "vite build --outDir dist/server --ssr src/entry-server.tsx",
    "serve:ssr": "set NODE_ENV=production ts-node server.ts"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.9.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/express": "^4.17.17",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.29.8",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gh-pages": "^5.0.0",
    "jsdom": "^21.1.1",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.6",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vitest": "^0.29.8"
  }
}
